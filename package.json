{
  "name": "monaco-jsx-highlighter",
  "version": "2.0.2",
  "description": "An extensible library to highlight (and comment) JSX syntax in the Monaco Editor using Babel.",
  "author": "luminaxster",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luminaxster/syntax-highlighter/issues"
  },
  "homepage": "https://luminaxster.github.io/syntax-highlighter/",
  "main": "dist/cjs/monaco-jsx-highlighter.js",
  "module": "dist/es/monaco-jsx-highlighter.js",
  "scripts": {
    "build-dev": "webpack --env development",
    "build": "rollup -c --environment INCLUDE_DEPS,BUILD:production",
    "clean": "rimraf dist",
    "lint": "eslint src --fix",
    "prerelease": "npm run lint && npm run test && npm run clean && npm run build",
    "release": "npm publish . --access public",
    "test": "echo \"Warning: no test specified\" && exit 0"
  },
  "peerDependencies": {
    "@babel/parser": "6.x || 7.x",
    "@babel/traverse": "6.x || 7.x",
    "monaco-editor": ">=0.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/parser": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.2.0",
    "@babel/traverse": "^7.2.0",
    "@babel/eslint-parser": "^7.16.5",
    "babel-loader": "8.2.3",
    "css-loader": "^5.0.0",
    "eslint": "^8.0.0",
    "monaco-editor": "^0.30.1",
    "react-refresh": "^0.11.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.64.0",
    "rollup-plugin-babel": "^5.0.0-alpha.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "schema-utils": "^3.1.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.0.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luminaxster/syntax-highlighter.git"
  },
  "keywords": [
    "monaco",
    "editor",
    "babel",
    "jsx",
    "syntax",
    "color",
    "coding",
    "highlighting"
  ],
  "browserslist":  "> 0.25%, not dead",
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  }
}
